{
  "name": "iptv",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "depcheck": "^1.4.7",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.9",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "version": "1.0.0",
  "dependencies": {
    "expo": "~54.0.30",
    "react": "19.1.0",
    "react-native": "0.81.5"
  }
}
